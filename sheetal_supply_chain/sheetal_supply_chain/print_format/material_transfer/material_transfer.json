 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html>\n<head>\n    <meta charset=\"utf-8\">\n    <style>\n        @media print {\n            @page {\n                size: A4;\n                margin: 15mm;\n            }\n            .items-table {\n                page-break-inside: auto;\n            }\n            .items-table tr {\n                page-break-inside: avoid;\n                page-break-after: auto;\n            }\n            .items-table thead {\n                display: table-header-group;\n            }\n        }\n        \n        body {\n            font-family: 'Arial', sans-serif;\n            font-size: 11pt;\n            margin: 0;\n            padding: 0;\n            color: #000;\n        }\n        \n        table {\n            width: 100%;\n            border-collapse: collapse;\n        }\n        \n        th, td {\n            border: 1px solid #000;\n            padding: 5px;\n            vertical-align: middle;\n        }\n        \n        th {\n            background-color: #f0f0f0;\n            text-align: center;\n            font-weight: bold;\n            font-size: 10pt;\n        }\n        \n        .header-cell {\n            border: 1px solid #000;\n            text-align: center;\n            padding: 15px 0;\n        }\n        \n        .company-name {\n            font-size: 18pt;\n            font-weight: bold;\n            text-transform: uppercase;\n            margin-bottom: 5px;\n        }\n        \n        .address {\n            font-size: 9pt;\n            margin-bottom: 8px;\n        }\n        \n        .document-title {\n            font-size: 14pt;\n            font-weight: bold;\n            text-transform: uppercase;\n        }\n        \n        .info-row td {\n            vertical-align: top;\n            padding: 8px;\n            font-size: 10pt;\n        }\n        \n        .detail-cell {\n            width: 16.66%;\n        }\n        \n        .items-table {\n            margin-top: 0;\n        }\n        \n        .items-table th {\n            padding: 8px;\n            height: 35px;\n            line-height: 35px;\n        }\n        \n        .items-table td {\n            padding: 6px;\n            font-size: 10pt;\n            text-align: center;\n        }\n        \n        .text-center {\n            text-align: center;\n        }\n        \n        .text-right {\n            text-align: right;\n        }\n        \n        .no-data {\n            text-align: center;\n            padding: 20px;\n            color: #666;\n            font-style: italic;\n        }\n        \n        .footer {\n            margin-top: 20px;\n            padding: 10px;\n            border-top: 1px solid #000;\n            font-size: 9pt;\n            text-align: center;\n        }\n    </style>\n</head>\n<body>\n    {% set company_doc = frappe.get_doc('Company', doc.company) %}\n    \n    <table>\n        <!-- Header / Letterhead -->\n        <tr>\n            <td colspan=\"6\" style=\"padding:0px !important;border:1px solid black;\">\n                <table style=\"width:100%; border:none; border-collapse:collapse;\">\n                    <tr>\n                        <td style=\"width:80px; vertical-align:top; padding:15px 10px; border:none;\">\n                            {% if company_doc.logo_for_printing %}\n                                <img src=\"{{ company_doc.logo_for_printing }}\" style=\"width:70px; height:auto;\">\n                            {% endif %}\n                        </td>\n                        <td style=\"padding-top: 10px !important; vertical-align:top; text-align:center; border:none;\">\n                            <div class=\"company-name\">{{ company_doc.company_name or doc.company }}</div>\n                           <div class=\"address\">\n  \n{% set company_address_name = frappe.db.get_value(\n    \"Dynamic Link\",\n    {\n        \"link_doctype\": \"Company\",\n        \"link_name\": doc.company,\n        \"parenttype\": \"Address\"\n    },\n    \"parent\"\n) %}\n\n\n\n{% if company_address_name %}\n    {% set company_address = frappe.get_doc(\"Address\", company_address_name) %}\n{% else %}\n    {% set company_address = None %}\n{% endif %}\n\n\n{% if company_address and company_address.custom_district %}\n    {% set district = frappe.db.get_value(\n        \"District\",\n        company_address.custom_district,\n    ) %}\n{% else %}\n    {% set district = \"\" %}\n{% endif %}\n\n{% set tel_no = frappe.db.get_value(\"Company\", doc.company, \"phone_no\") %}\n{% set company_email = frappe.db.get_value(\"Company\", doc.company, \"email\") %}\n{% set company_website = frappe.db.get_value(\"Company\", doc.company, \"website\") %}\n\n{% if company_address %}\n\n    {{ company_address.address_line1 or \"\" }}\n    {{ company_address.address_line2 or \"\" }}<br>\n\n    {{ district or \"\" }}\n    {% if district %}, {% endif %}\n    {{ company_address.state or \"\" }}\n    {% if company_address.state %}, {% endif %}\n    {{ company_address.country or \"\" }}\n    {{ company_address.pincode or \"\" }}\n\n    <br>\n\n    Tel: {{ tel_no or \"\" }},\n    <span style=\"margin-left:5px!important;\">\n        Email ID: {{ company_email or \"\" }},\n    </span>\n    <span style=\"margin-left:5px!important;\">\n        Website: {{ company_website or \"\" }}\n    </span>\n\n{% endif %}\n\n \n</div>\n </td>\n    </tr>\n        </table>\n            </td>\n        </tr>\n        \n        <!-- Document Title Row -->\n        <tr>\n            <td colspan=\"6\" class=\"header-cell\" style=\"padding: 10px;\">\n                <div class=\"document-title\">MATERIAL TRANSFER</div>\n            </td>\n        </tr>\n        \n        <!-- Material Transfer Info Section -->\n        <tr class=\"info-row\">\n            <td colspan=\"3\" class=\"detail-cell\">\n                <strong>Stock Entry ID</strong>: {{ doc.name }}\n            </td>\n            <td colspan=\"3\" class=\"detail-cell\">\n                <strong>Posting Date</strong>: {{ frappe.utils.formatdate(doc.posting_date, \"dd-MM-yyyy\") }}\n            </td>\n        </tr>\n        <tr class=\"info-row\">\n           <td colspan=\"3\" class=\"detail-cell\">\n    <strong>Posting Time</strong>:\n   {{ doc.posting_time.split(\".\")[0] if doc.posting_time else \"-\" }}\n</td>\n\n            <td colspan=\"3\" class=\"detail-cell\">\n                <strong>Status</strong>: {{ doc.docstatus == 0 and \"Draft\" or (doc.docstatus == 1 and \"Submitted\" or \"Cancelled\") }}\n            </td>\n        </tr>\n        \n        {% if doc.from_warehouse or doc.to_warehouse %}\n        <tr class=\"info-row\">\n            {% if doc.from_warehouse %}\n            <td colspan=\"3\" class=\"detail-cell\">\n                <strong>From Warehouse</strong>: {{ doc.from_warehouse }}\n            </td>\n            {% else %}\n            <td colspan=\"3\" class=\"detail-cell\"></td>\n            {% endif %}\n            {% if doc.to_warehouse %}\n            <td colspan=\"3\" class=\"detail-cell\">\n                <strong>To Warehouse</strong>: {{ doc.to_warehouse }}\n            </td>\n            {% else %}\n            <td colspan=\"3\" class=\"detail-cell\"></td>\n            {% endif %}\n        </tr>\n        {% endif %}\n    </table>\n    \n    <!-- Material Transfer Items Table -->\n    <table>\n        <thead>\n        <tr>\n            <th style=\"width: 8%;\">SR No</th>\n            <th style=\"width: 20%;\">Item Code</th>\n            <th style=\"width: 28%;\">Item Name</th>\n            <th style=\"width: 18%;\">Source Warehouse</th>\n            <th style=\"width: 18%;\">Target Warehouse</th>\n            <th style=\"width: 8%;\">Qty</th>\n        </tr>\n        </thead>\n        \n        <!-- Material Transfer Items Body -->\n        <tbody>\n        {% if doc.items %}\n            {% for item in doc.items %}\n            <tr class=\"items-table\">\n                <td class=\"text-center\">{{ loop.index }}</td>\n                <td class=\"text-center\">{{ item.item_code or \"-\" }}</td>\n                <td>{{ item.item_name or \"-\" }}</td>\n                <td class=\"text-center\">{{ item.s_warehouse or \"-\" }}</td>\n                <td class=\"text-center\">{{ item.t_warehouse or \"-\" }}</td>\n                <td class=\"text-right\">{{ \"%.3f\"|format(item.qty or 0) }}</td>\n            </tr>\n            {% endfor %}\n        {% else %}\n            <tr>\n                <td colspan=\"6\" class=\"no-data\">No items found for this material transfer.</td>\n            </tr>\n        {% endif %}\n        </tbody>\n    </table>\n\n</body>\n</html>",
