 "html": "<!DOCTYPE html>\n<html>\n<head>\n<meta charset=\"utf-8\">\n<style>\n        @media print {\n            @page {\n                size: A4;\n                margin: 15mm;\n            }\n            .items-table {\n                page-break-inside: auto;\n            }\n            .items-table tr {\n                page-break-inside: avoid;\n                page-break-after: auto;\n            }\n            .items-table thead {\n                display: table-header-group;\n            }\n        }\n        body {\n            font-family: 'Arial', sans-serif;\n            font-size: 11pt;\n            margin: 0;\n            padding: 0;\n            color: #000;\n        }\n        table {\n            width: 100%;\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid #000;\n            padding: 5px;\n            vertical-align: middle;\n        }\n        th {\n            background-color: #f0f0f0;\n            text-align: center;\n            font-weight: bold;\n            font-size: 10pt;\n        }\n        .header-cell {\n            border: 1px solid #000;\n            text-align: center;\n            padding: 15px 0;\n        }\n        .company-name {\n            font-size: 18pt;\n            font-weight: bold;\n            text-transform: uppercase;\n            margin-bottom: 5px;\n        }\n        .address {\n            font-size: 9pt;\n            margin-bottom: 8px;\n        }\n        .document-title {\n            font-size: 14pt;\n            font-weight: bold;\n            text-transform: uppercase;\n        }\n        .info-row td {\n            vertical-align: top;\n            padding: 8px;\n            font-size: 10pt;\n        }\n        .detail-cell {\n            width: 16.66%;\n        }\n        .items-table {\n            margin-top: 0;\n        }\n        .items-table th {\n            padding: 8px;\n            height: 35px;\n            line-height: 35px;\n        }\n        .items-table td {\n            padding: 6px;\n            font-size: 10pt;\n            text-align: center;\n        }\n        .text-center {\n            text-align: center;\n        }\n        .text-right {\n            text-align: right;\n        }\n        .no-data {\n            text-align: center;\n            padding: 20px;\n            color: #666;\n            font-style: italic;\n        }\n        .footer {\n            margin-top: 20px;\n            padding: 10px;\n            border-top: 1px solid #000;\n            font-size: 9pt;\n            text-align: center;\n        }\n</style>\n</head>\n<body>\n    {% set company_doc = frappe.get_doc('Company', doc.company) %}\n<table>\n<!-- Header / Letterhead -->\n<tr>\n<td colspan=\"6\" style=\"padding:0px !important;border:1px solid black;\">\n<table style=\"width:100%; border:none; border-collapse:collapse;\">\n<tr>\n<td style=\"width:80px; vertical-align:top; padding:15px 10px; border:none;\">\n                            {% if company_doc.logo_for_printing %}\n<img src=\"{{ company_doc.logo_for_printing }}\" style=\"width:70px; height:auto;\">\n                            {% endif %}\n</td>\n<td style=\"padding-top: 10px !important; vertical-align:top; text-align:center; border:none;\">\n<div class=\"company-name\">{{ company_doc.company_name or doc.company }}</div>\n<div class=\"address\">\n\n\n{% set company_address_name = frappe.db.get_value(\n    \"Dynamic Link\",\n    {\n        \"link_doctype\": \"Company\",\n        \"link_name\": doc.company,\n        \"parenttype\": \"Address\"\n    },\n    \"parent\"\n) %}\n\n\n{% if company_address_name %}\n    {% set company_address = frappe.get_doc(\"Address\", company_address_name) %}\n{% else %}\n    {% set company_address = None %}\n{% endif %}\n\n{% if company_address and company_address.custom_district %}\n    {% set district = frappe.db.get_value(\n        \"District\",\n        company_address.custom_district,\n        \"district_name\"\n    ) %}\n{% else %}\n    {% set district = \"\" %}\n{% endif %}\n\n\n{% set tel_no = frappe.db.get_value(\"Company\", doc.company, \"phone_no\") %}\n{% set company_email = frappe.db.get_value(\"Company\", doc.company, \"email\") %}\n{% set company_website = frappe.db.get_value(\"Company\", doc.company, \"website\") %}\n\n\n\n{% if company_address %}\n\n    {{ company_address.address_line1 or \"\" }}\n    {{ company_address.address_line2 or \"\" }}<br>\n\n    {{ district or \"\" }}\n    {% if district %}, {% endif %}\n    {{ company_address.state or \"\" }}\n    {% if company_address.state %}, {% endif %}\n    {{ company_address.country or \"\" }}\n    {{ company_address.pincode or \"\" }}\n\n    <br>\n\n    Tel: {{ tel_no or \"\" }},\n    <span style=\"margin-left:5px!important;\">\n        Email ID: {{ company_email or \"\" }},\n    </span>\n    <span style=\"margin-left:5px!important;\">\n        Website: {{ company_website or \"\" }}\n    </span>\n\n{% endif %}\n\n\n</div>\n</td>\n</tr>\n</table>\n</td>\n</tr>\n<!-- Document Title Row -->\n<tr>\n<td colspan=\"6\" class=\"header-cell\" style=\"padding: 10px;\">\n<div class=\"document-title\">WORK ORDER</div>\n</td>\n</tr>\n<!-- Work Order Info Section -->\n<tr class=\"info-row\">\n<td colspan=\"3\" class=\"detail-cell\">\n<strong>Item Code</strong>: {{ doc.production_item or \"-\" }}\n</td>\n<td colspan=\"3\" class=\"detail-cell\">\n<strong>Item Name</strong>: {{ doc.item_name or \"-\" }}\n</td>\n</tr>\n<tr class=\"info-row\">\n<td colspan=\"3\" class=\"detail-cell\">\n<strong>BOM No</strong>: {{ doc.bom_no or \"-\" }}\n</td>\n<td colspan=\"3\" class=\"detail-cell\">\n<strong>Sales Order</strong>: {{ doc.sales_order or \"\" }}\n</td>\n</tr>\n<tr class=\"info-row\">\n<td colspan=\"3\" class=\"detail-cell\">\n<strong>Target Warehouse</strong>: {{ doc.fg_warehouse or \"-\" }}\n</td>\n<td colspan=\"3\" class=\"detail-cell\">\n<strong>Quantity to Manufacture</strong>: {{ doc.qty or \"0\" }}\n</td>\n</tr>\n<!-- Work Order Details Row -->\n<tr class=\"info-row\">\n<td colspan=\"3\" class=\"detail-cell\">\n<strong>Work Order No.</strong>: {{ doc.name }}\n</td>\n<td colspan=\"3\" class=\"detail-cell\">\n                {% if doc.expected_delivery_date %}\n<strong>Expected Delivery Date</strong>: {{ frappe.utils.formatdate(doc.expected_delivery_date, \"dd-MM-yyyy\") }}\n                {% endif %}\n                {% if doc.sales_order %}\n<br><strong>Sales Order</strong>: {{ doc.sales_order }}\n                {% endif %}\n</td>\n</tr>\n</table>\n<!-- Required Items Table -->\n<table>\n<thead>\n<tr>\n<th style=\"width: 8%;\">SR No</th>\n<th style=\"width: 22%;\">Item Code</th>\n<th style=\"width: 30%;\">Item Name</th>\n<th style=\"width: 20%;\">Source Warehouse</th>\n<th style=\"width: 12%;\">Required Qty</th>\n<th style=\"width: 8%;\">UOM</th>\n</tr>\n</thead>\n<!-- Required Items Body -->\n<tbody>\n        {% if doc.required_items %}\n            {% for item in doc.required_items %}\n<tr class=\"items-table\">\n<td class=\"text-center\">{{ loop.index }}</td>\n<td class=\"text-center\">{{ item.item_code or \"-\" }}</td>\n<td>{{ item.item_name or \"-\" }}</td>\n<td class=\"text-center\">{{ item.source_warehouse or \"-\" }}</td>\n<td class=\"text-right\">{{ \"%.3f\"|format(item.required_qty or 0) }}</td>\n<td class=\"text-center\">{{ item.stock_uom or \"-\" }}</td>\n</tr>\n            {% endfor %}\n        {% else %}\n<tr>\n<td colspan=\"6\" class=\"no-data\">No required items found for this work order.</td>\n</tr>\n        {% endif %}\n</tbody>\n</table>\n<!-- Expected Fat & SNF and Footer Table -->\n<table>\n<!-- Expected Fat & SNF Section (if applicable) -->\n        {% if doc.expected_fat_percent or doc.expected_snf_percent %}\n<tr>\n<td colspan=\"6\" style=\"padding: 10px; font-weight: bold; background-color: #f0f0f0;\">\n                Expected Fat & SNF\n</td>\n</tr>\n<tr class=\"info-row\">\n            {% if doc.expected_fat_percent %}\n<td colspan=\"3\">\n<strong>Expected Fat %</strong>: {{ \"%.2f\"|format(doc.expected_fat_percent) }}%\n</td>\n            {% endif %}\n            {% if doc.expected_snf_percent %}\n<td colspan=\"3\">\n<strong>Expected SNF %</strong>: {{ \"%.2f\"|format(doc.expected_snf_percent) }}%\n</td>\n            {% endif %}\n</tr>\n        {% endif %}\n\n</table>\n</body>\n</html>",
